#include "fonctions.h"
#include "my_dico.h"
#include <stdlib.h>

int	isalphanum(char *str)
{
  int	i;
  int	count;

  i = 0;
  count = 0;
  while (str[i] != '\0')
    {
      if ((str[i] >= '0' && str[i] <= '9') ||
	  (str[i] >= 'a' && str[i] <= 'z') ||
	  (str[i] >= 'A' && str[i] <= 'Z'))
	count += 0;
      else 
	count++;
      i++;
    }
  return (count);
}

